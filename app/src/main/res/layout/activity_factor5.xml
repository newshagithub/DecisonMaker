<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_factor5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.newsha.decisonmaker.Factor5">

    <TextView
        android:text="@string/factor1_intro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="45dp"
        android:id="@+id/textView33"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <TextView
        android:text="@string/emptyString"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView33"
        android:layout_alignLeft="@+id/textView33"
        android:layout_alignStart="@+id/textView33"
        android:id="@+id/TVfactor5"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <TextView
        android:text="1. "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/TVfactor5"
        android:layout_alignLeft="@+id/TVfactor5"
        android:layout_alignStart="@+id/TVfactor5"
        android:layout_marginTop="75dp"
        android:id="@+id/textView41"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <TextView
        android:text="@string/emptyString"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView41"
        android:layout_toRightOf="@+id/textView41"
        android:layout_toEndOf="@+id/textView41"
        android:id="@+id/option1"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratef5_op1"
        style="@android:style/Widget.Holo.Light.RatingBar.Small"
        android:layout_alignBottom="@+id/option1"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="18dp"
        android:layout_marginEnd="18dp" />

    <TextView
        android:text="2. "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView41"
        android:layout_alignLeft="@+id/textView41"
        android:layout_alignStart="@+id/textView41"
        android:layout_marginTop="28dp"
        android:id="@+id/TextView42"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <TextView
        android:text="@string/emptyString"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/TextView42"
        android:layout_toRightOf="@+id/TextView42"
        android:layout_toEndOf="@+id/TextView42"
        android:id="@+id/option2"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratef5_op2"
        style="@android:style/Widget.Holo.Light.RatingBar.Small"
        android:layout_alignTop="@+id/option2"
        android:layout_alignLeft="@+id/ratef5_op1"
        android:layout_alignStart="@+id/ratef5_op1" />

    <TextView
        android:text="3. "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView48"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:layout_below="@+id/TextView42"
        android:layout_alignRight="@+id/option2"
        android:layout_alignEnd="@+id/option2"
        android:layout_marginTop="29dp" />

    <TextView
        android:text="@string/emptyString"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView48"
        android:layout_toRightOf="@+id/option2"
        android:layout_toEndOf="@+id/option2"
        android:id="@+id/option3"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratef5_op3"
        style="@android:style/Widget.Holo.Light.RatingBar.Small"
        android:layout_alignBottom="@+id/option3"
        android:layout_alignLeft="@+id/ratef5_op2"
        android:layout_alignStart="@+id/ratef5_op2" />

    <TextView
        android:text="4. "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView48"
        android:layout_toRightOf="@+id/TVfactor5"
        android:layout_toEndOf="@+id/TVfactor5"
        android:layout_marginTop="30dp"
        android:id="@+id/textView50"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <TextView
        android:text="@string/emptyString"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView50"
        android:layout_toRightOf="@+id/option3"
        android:layout_toEndOf="@+id/option3"
        android:id="@+id/option4"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratef5_op4"
        style="@android:style/Widget.Holo.Light.RatingBar.Small"
        android:layout_alignBottom="@+id/option4"
        android:layout_alignLeft="@+id/ratef5_op3"
        android:layout_alignStart="@+id/ratef5_op3" />

    <TextView
        android:text="@string/emptyString"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView53"
        android:layout_alignBaseline="@+id/textView52"
        android:layout_alignBottom="@+id/textView52"
        android:layout_toRightOf="@+id/option4"
        android:layout_toEndOf="@+id/option4"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <TextView
        android:text="5. "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="31dp"
        android:id="@+id/textView52"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:layout_below="@+id/textView50"
        android:layout_toLeftOf="@+id/textView53"
        android:layout_toStartOf="@+id/textView53" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratef5_op5"
        style="@android:style/Widget.Holo.Light.RatingBar.Small"
        android:layout_alignTop="@+id/textView53"
        android:layout_alignLeft="@+id/ratef5_op4"
        android:layout_alignStart="@+id/ratef5_op4" />

    <Button
        android:text="next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView53"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="22dp"
        android:id="@+id/f5next-btn" />
</RelativeLayout>
