        android:layout_marginTop="21dp"
        android:id="@+id/textView38" />

    <TextView
        android:text="@string/emptyString"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView38"
