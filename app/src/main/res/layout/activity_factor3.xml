    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/rateF3O1"
        android:layout_alignBaseline="@+id/TVoption1"
        android:layout_alignBottom="@+id/TVoption1"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="18dp"
        android:layout_marginEnd="18dp"
        android:layout_alignLeft="@+id/textView11"
        android:layout_alignStart="@+id/textView11" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:layout_below="@+id/rateF3O1"
        android:layout_alignRight="@+id/rateF3O1"
        android:layout_alignEnd="@+id/rateF3O1"
        android:id="@+id/rateF3O2"
        android:layout_alignLeft="@+id/rateF3O1"
        android:layout_alignStart="@+id/rateF3O1" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:layout_below="@+id/rateF3O2"
        android:layout_alignRight="@+id/rateF3O2"
        android:layout_alignEnd="@+id/rateF3O2"
