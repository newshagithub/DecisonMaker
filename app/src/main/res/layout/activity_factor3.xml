        android:layout_alignStart="@+id/rateF3O2" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:layout_below="@+id/rateF3O3"
        android:layout_alignRight="@+id/rateF3O3"
        android:layout_alignEnd="@+id/rateF3O3"
        android:id="@+id/rateF3O4"
        android:layout_alignLeft="@+id/rateF3O3"
        android:layout_alignStart="@+id/rateF3O3" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/rateF3O5"
        android:layout_alignBaseline="@+id/TVoption5"
        android:layout_alignBottom="@+id/TVoption5"
        android:layout_alignRight="@+id/rateF3O4"
        android:layout_alignEnd="@+id/rateF3O4"
        android:layout_alignLeft="@+id/rateF3O4"
        android:layout_alignStart="@+id/rateF3O4" />

</RelativeLayout>
