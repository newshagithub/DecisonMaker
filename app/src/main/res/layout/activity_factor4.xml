<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_factor4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.newsha.decisonmaker.Factor4">

    <TextView
        android:text="@string/factor1_intro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="47dp"
        android:id="@+id/textView31"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <TextView
        android:text="@string/emptyString"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView31"
        android:layout_alignLeft="@+id/textView31"
        android:layout_alignStart="@+id/textView31"
        android:id="@+id/TVfactor4" />

    <TextView
        android:text="1. "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/TVfactor4"
        android:layout_toRightOf="@+id/TVfactor4"
        android:layout_toEndOf="@+id/TVfactor4"
        android:layout_marginTop="64dp"
        android:id="@+id/textView35"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <TextView
        android:text="@string/emptyString"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView35"
        android:layout_toRightOf="@+id/textView35"
        android:layout_toEndOf="@+id/textView35"
        android:id="@+id/option1"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratef4_op1"
        style="@android:style/Widget.Holo.Light.RatingBar.Small"
        android:layout_alignBottom="@+id/option1"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="22dp"
        android:layout_marginEnd="22dp" />

    <TextView
        android:text="2. "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:id="@+id/textView39"
        android:layout_below="@+id/textView35"
        android:layout_toRightOf="@+id/TVfactor4"
        android:layout_toEndOf="@+id/TVfactor4"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <TextView
        android:text="@string/emptyString"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView39"
        android:layout_toRightOf="@+id/option1"
        android:layout_toEndOf="@+id/option1"
        android:id="@+id/option2"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <TextView
        android:text="3. "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/TVfactor4"
        android:layout_toEndOf="@+id/TVfactor4"
        android:id="@+id/textView42"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratef4_op2"
        style="@android:style/Widget.Holo.Light.RatingBar.Small"
        android:layout_alignTop="@+id/option2"
        android:layout_alignLeft="@+id/ratef4_op1"
        android:layout_alignStart="@+id/ratef4_op1" />

    <TextView
        android:text="@string/emptyString"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/option2"
        android:layout_toEndOf="@+id/option2"
        android:id="@+id/option3"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratef3_op3"
        style="@android:style/Widget.Holo.Light.RatingBar.Small"
        android:layout_alignTop="@+id/option3"
        android:layout_alignLeft="@+id/ratef4_op2"
        android:layout_alignStart="@+id/ratef4_op2" />

    <TextView
        android:text="@string/emptyString"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView44"
        android:layout_toRightOf="@+id/option3"
        android:layout_toEndOf="@+id/option3"
        android:id="@+id/option4"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratef4_op4"
        style="@android:style/Widget.Holo.Light.RatingBar.Small"
        android:layout_alignBottom="@+id/option4"
        android:layout_alignLeft="@+id/ratef3_op3"
        android:layout_alignStart="@+id/ratef3_op3" />

    <TextView
        android:text="4. "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="21dp"
        android:id="@+id/textView44"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:layout_below="@+id/textView42"
        android:layout_toLeftOf="@+id/option4"
        android:layout_toStartOf="@+id/option4" />

    <TextView
        android:text="5. "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="27dp"
        android:id="@+id/textView45"
        android:layout_below="@+id/option4"
        android:layout_alignLeft="@+id/textView44"
        android:layout_alignStart="@+id/textView44"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <TextView
        android:text="@string/emptyString"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView45"
        android:layout_toRightOf="@+id/textView45"
        android:layout_toEndOf="@+id/textView45"
        android:id="@+id/option5"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratef4_op5"
        style="@android:style/Widget.Holo.Light.RatingBar.Small"
        android:layout_alignBottom="@+id/option5"
        android:layout_alignLeft="@+id/ratef4_op4"
        android:layout_alignStart="@+id/ratef4_op4" />

    <Button
        android:text="next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:id="@+id/f4next_btn"
        android:layout_below="@+id/ratef4_op5"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
